function listbox_moveacross(sourceID,destID){var src=document.getElementById(sourceID);var dest=document.getElementById(destID);for(var count=0;count<src.options.length;count++){if(src.options[count].selected==true){var option=src.options[count];var newOption=document.createElement("option");newOption.value=option.value;newOption.text=option.text;newOption.title=option.title;newOption.selected=true;try{dest.add(newOption,null);src.remove(count,null)}catch(error){dest.add(newOption);src.remove(count)}count--}}var countreduced=false;for(var destcount=0;destcount<dest.options.length-1;destcount++){var option=dest.options[destcount];for(destcounttwo=destcount+1;destcounttwo<dest.options.length;destcounttwo++){var destoption=dest.options[destcounttwo];if(option.value==destoption.value){try{dest.remove(destcount,null);countreduced=true}catch(error){dest.remove(destcount)}}}if(countreduced==true){destcount--;countreduced=false}}}function listbox_moveacross_selected(sourceID,destID){var src=document.getElementById(sourceID);var dest=document.getElementById(destID);for(var count=0;count<src.options.length;count++){if(src.options[count].selected==true){var option=src.options[count];var newOption=document.createElement("option");newOption.value=option.value;newOption.text=option.text;newOption.selected=true;try{dest.add(newOption,null);break}catch(error){dest.add(newOption);break}count--}}}function listbox_moveacross_left(sourceID,destID){var src=document.getElementById(sourceID);var dest=document.getElementById(destID);var repeatedvariable=false;for(var count=0;count<src.options.length;count++){if(src.options[count].selected==true){var option=src.options[count];for(var destcount=0;destcount<dest.options.length;destcount++){var destoption=dest.options[destcount];if(option.value==destoption.value){repeatedvariable=true;try{src.remove(count,null)}catch(error){src.remove(count)}count--}}}}if(false==repeatedvariable){for(var count=1;count<src.options.length;count++){if(src.options[count].selected==true){var option=src.options[count];var newOption=document.createElement("option");newOption.value=option.value;newOption.text=option.text;newOption.selected=true;try{dest.add(newOption,null);src.remove(count,null)}catch(error){dest.add(newOption);src.remove(count)}count--}}}}function notEmpty(elem){if(elem.value.length==0){elem.focus();return false}return true}function isAlphabet(elem){var alphaExp=/^[a-zA-Z ]+$/;if(elem.value.match(alphaExp)){return true}else{elem.focus();return false}}function selectAll(selectBox,selectAll){if(typeof selectBox=="string"){selectBox=document.getElementById(selectBox)}if(selectBox.type=="select-multiple"){for(var i=0;i<selectBox.options.length;i++){selectBox.options[i].selected=selectAll}}}function readMoreEconomicData(readMoreClick,div,size,threshold){if(jQuery("#"+readMoreClick).html().indexOf("More")!=-1){medCounter=medCounter+1;var s=(medCounter*threshold)+1;var e=(medCounter+1)*threshold;if(e>size){e=size}while(s<=e){jQuery("#"+div+s).show();s=s+1}if(e==size){jQuery("#"+readMoreClick).html("Less");medCounter=0}}else{medCounter=0;var s=((medCounter+1)*threshold)+1;var e=size;while(s<=e){jQuery("#"+div+s).hide();s=s+1}jQuery("#"+readMoreClick).html("More")}};